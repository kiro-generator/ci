{
  "name": "@dougefresh/ci",
  "description": "CI Mega Config github action",
  "version": "0.1.17",
  "author": "",
  "type": "module",
  "homepage": "https://github.com/dougefresh/ci",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dougefresh/ci.git"
  },
  "keywords": [
    "actions"
  ],
  "files": [
    "dist",
    "src",
    "tsconfig.json"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "precommit": "rm -rf dist && bun run lint:fix && bun run build && bun run check:configs",
    "check:configs": "tsgo --noEmit -p tsconfig-config.json",
    "lint:fix": "biome check --fix && biome format --write ",
    "lint": "biome check",
    "build": "tsgo",
    "prepublishOnly": "bun run build"
  },
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "@types/node": "^24.10.1",
    "@typescript/native-preview": "latest"
  }
}
